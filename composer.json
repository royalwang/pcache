{
    "name": "soranoba/pcache",
    "description": "Cache library wrapper that supports multiple middleware (Shared Memory, Redis, Memcached) with PHP.",
    "keywords": ["pcache", "cache", "redis", "memcached"],
    "type": "library",
    "homepage": "http://github.com/soranoba/pcache",
    "license": "MIT",
    "authors": [
        {
            "name": "Hinagiku Soranoba",
            "email": "soranoba@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5",
        "ext-redis": ">=2.2",
        "ext-memcached": ">=2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "pcache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\pcache\\": "tests/"
        }
    }
}
