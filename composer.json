{
    "name": "soranoba/pcache",
    "description": "Cache library that supports multiple middleware",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6",
        "ext-redis": ">=2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6",
        "squizlabs/php_codesniffer": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "pcache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\pcache\\": "tests/"
        }
    }
}
